issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
keywords = {fluid flows,particle,river dynamics,sediment transport},
pages = {893--903},
title = {{Theoretical impulse threshold for particle dislodgement}},
url = {http://arxiv.org/abs/1901.10210{\%}0Ahttp://dx.doi.org/10.1017/jfm.2018.996},
volume = {863},
year = {2019}
