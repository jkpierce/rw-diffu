issn = {17425662},
journal = {Journal of the Royal Society Interface},
keywords = {Anomalous diffusion,Bee-mediated pollen dispersal,Genetically modified crops,Isolation distances,Truncated L{\'{e}}vy flights},
number = {126},
title = {{A L{\'{e}}vy-flight diffusion model to predict transgenic pollen dispersal}},
volume = {14},
