doi = {10.1103/PhysRevE.91.042115},
file = {:home/kpierce/Documents/mendeley/Sim, Liepe, Stumpf - 2015 - Goldstein-Kac telegraph processes with random speeds Path probabilities, likelihoods, and reported L{\'{e}}vy fli.pdf:pdf},
