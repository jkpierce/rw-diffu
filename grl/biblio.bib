@article{Turowski2009,
abstract = {Several important fluvial bedrock erosion processes are driven by{\$}\backslash{\$}nthe impact of bed load particles. Bed load transport rates fluctuate{\$}\backslash{\$}nstrongly in both nature and experiment, and stochastic models of{\$}\backslash{\$}nthe transport processes have been put forward to describe this behavior.{\$}\backslash{\$}nIn this paper I adapt a model based on a Markov chain formulation{\$}\backslash{\$}nto derive probability distributions for bed load transport rate over{\$}\backslash{\$}na rock bed only partly covered by sediment. I propose a way to calculate{\$}\backslash{\$}nthe probability distribution of bed cover for given sediment supply{\$}\backslash{\$}nusing a combinatoric model and combine the two curves to calculate{\$}\backslash{\$}nprobability distributions for bed cover and erosion rate at constant{\$}\backslash{\$}nhydraulics. In the proposed model, mean bed cover is an exponentially{\$}\backslash{\$}ndeclining function of the number of particles in the control volume.{\$}\backslash{\$}nThe model describes recently published experimental data well, but{\$}\backslash{\$}nat the moment it is not possible to finally discriminate between{\$}\backslash{\$}nthe exponential and the previously proposed linear model formulation.{\$}\backslash{\$}nDistributions of erosion rate are fairly broad functions slightly{\$}\backslash{\$}nskewed toward high erosion rates.},
