author = {Dey, Subhasish and Ali, Sk Zeeshan},
doi = {10.1063/1.4984042},
file = {:home/kpierce/Documents/mendeley/Dey, Ali - 2017 - Stochastic mechanics of loose boundary particle transport in turbulent flow.pdf:pdf},
issn = {10897666},
journal = {Physics of Fluids},
number = {5},
title = {{Stochastic mechanics of loose boundary particle transport in turbulent flow}},
volume = {29},
