isbn = {1434-5021},
issn = {14345021},
journal = {Granular Matter},
keywords = {DEM,Granular media,Particle packing,Particle shape,Voronoi tessellation},
number = {5},
pages = {621--638},
pmid = {21971955},
title = {{Fourier-Voronoi-based generation of realistic samples for discrete modelling of granular materials}},
volume = {14},
year = {2012}
}
