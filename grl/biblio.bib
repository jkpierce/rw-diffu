@article{Negro2013,
abstract = {The construction of ski-pistes can cause serious damage to all the{\$}\backslash{\$}ncomponents of an ecosystem and may therefore be considered as one of the{\$}\backslash{\$}nmain causes of human-driven environmental changes in mountain habitats.{\$}\backslash{\$}nThis study was aimed at assessing the responses of different ground{\$}\backslash{\$}ndwelling arthropod assemblages (i.e. ground beetles, spiders and{\$}\backslash{\$}ngrasshoppers) to ski-piste restoration through hydroseeding with{\$}\backslash{\$}ncommercial seed mixtures. The soil, vegetation and arthropods of{\$}\backslash{\$}nrestored and un-restored ski-pistes and the adjacent grassland have been{\$}\backslash{\$}nsampled at two high altitude sites in the north-western Italian Alps.{\$}\backslash{\$}nThe ski-piste soil was characterized by a higher skeletal content and,{\$}\backslash{\$}nconsequently, a lower amount of fine earth and organic matter than that{\$}\backslash{\$}nof the undisturbed adjacent grassland. The plant cover of the restored{\$}\backslash{\$}nski-pistes was very low above 2,500 m a.s.l.; the vegetation cover below{\$}\backslash{\$}nthis threshold was higher in the study area, mainly due to the seeded{\$}\backslash{\$}nspecies. The unrestored ski-pistes showed very large proportions of bare{\$}\backslash{\$}nground. The responses of the three ground-dwelling arthropod groups to{\$}\backslash{\$}nvegetation conditions were different. GLMMs and IndVal analyses showed{\$}\backslash{\$}nthat all three groups avoided the unrestored ski-pistes plots, which{\$}\backslash{\$}nwere characterized by a very scarce vegetation cover, irrespective of{\$}\backslash{\$}nthe altitude. The low altitude plots of the restored ski-pistes, which{\$}\backslash{\$}nwere characterized by a high vegetation cover, had a significantly{\$}\backslash{\$}nlarger number of grasshoppers on the ski-pistes than on the adjacent{\$}\backslash{\$}ngrassland plots. Moreover, these restored ski-piste plots were used in{\$}\backslash{\$}nequal proportion to the adjacent grassland plots by ground beetles but{\$}\backslash{\$}navoided by spiders. When ski-pistes become sufficiently revegetated by{\$}\backslash{\$}nhydroseeded plants, they are colonized by grasshoppers and, to a lesser{\$}\backslash{\$}nextent, by ground-beetles. Spiders, instead, do not colonize the pistes,{\$}\backslash{\$}nthus demonstrating that they are the most sensitive arthropods and may{\$}\backslash{\$}ntherefore represent the best indicators of human-driven environmental{\$}\backslash{\$}nchanges in high altitude alpine habitats.},
author = {Negro, Matteo and Rolando, Antonio and Barni, Elena and Bocola, Davide and Filippa, Gianluca and Freppaz, Michele and Isaia, Marco and Siniscalco, Consolata and Palestrini, Claudia},
