eprint = {0105071},
file = {:home/kpierce/Documents/mendeley/Silbert et al. - 2001 - Granular flow down an inclined plane Bagnold scaling and rheology.pdf:pdf},
isbn = {1063-651X},
issn = {1063651X},
journal = {Physical Review E - Statistical Physics, Plasmas, Fluids, and Related Interdisciplinary Topics},
number = {5},
pages = {14},
pmid = {11735913},
