@article{KOMAR1991,
abstract = {Flow-competence assessments of floods have been based on the largest$\backslash$nparticle sizes transported, and yield either the mean flow stress,$\backslash$nmean velocity, or discharge per unit flow width. The use of extreme$\backslash$nparticle sizes has potential problems in that they may have been$\backslash$ntransported by debris flows rather than by the flood, it may be difficult$\backslash$nto locate the largest particles within the flood deposits, and there$\backslash$nare questions concerning how representative one or a few large particles$\backslash$nmight be of the transported sediments and therefore of the flood$\backslash$nhydraulics. Such problems would be eliminated for the most part if$\backslash$ncompetence evaluations are based on median grain sizes of transported$\backslash$nsediments, or perhaps on some coarse percentile that is established$\backslash$nby a reasonable number of grains. In order to examine such issues,$\backslash$nthe gravel-transport data of Milhous from Oak Creek, Oregon, and$\backslash$nof Carling from Great Eggleshope Beck, England, have been analysed$\backslash$nin terms of changing grain-size percentiles with varying flow stresses.$\backslash$nA comparison between these two data sets is of added interest because$\backslash$nthe bed material in Oak Creek is segregated into welldeveloped pavement$\backslash$nand subpavement layers, while such a layering of bed materials is$\backslash$nlargely absent in Great Eggleshope Beck.$\backslash$n$\backslash$nThe analyses show that the trend of increasing sizes of the largest$\backslash$nparticles in the bedload samples (diameter D,) with increasing flow$\backslash$nstresses is consistent with similar dependencies based on sieve percentiles$\backslash$nranging from the medians (D50) to the 95th percentiles (D,,). This$\backslash$nindicates that the largest particles are an integral part of the$\backslash$noverall distributions of bedload grain sizes, and respond to changing$\backslash$nflow hydraulics along with the rest of the size distribution. In$\backslash$nOak Creek, the median grain size shows the largest change with increasing$\backslash$nflow stresses, followed by Dso, and so on to D,, which shows the$\backslash$nsmallest change. The variations in D, continue this trend, and are$\backslash$nsimilar to those for D95. This systematic variation of grain-size$\backslash$npercentiles in Oak Creek is consistent with changes in the overall$\backslash$ndistributions which tend to be symmetrical and Gaussian for low discharges,$\backslash$nbut become skewed Rosin distributions for high discharges. In contrast,$\backslash$nin Great Eggleshope Beck the several percentiles and D, show the$\backslash$nsame rate of shift to coarser sizes as flow stresses increase. This$\backslash$nresults in part from differences in sampling techniques wherein the$\backslash$nbedload samples from Great Eggleshope Beck represent a complete flood$\backslash$nevent, while shorterterm samples at a specific flow stage were obtained$\backslash$nin Oak Creek. As a result of the integrated sampling in Great Eggleshope$\backslash$nBeck, the bedload grain-size distributions are more complex, commonly$\backslash$nwith a bimodal pattern. However, after accounting for differences$\backslash$nin sampling schemes in the two streams, contrasting patterns in changing$\backslash$ngrain-size distributions remain, and these are concluded to reflect$\backslash$ngrain sorting differences as the bedload grain-size distributions$\backslash$napproach the distributions of the bed materials. It is surprising$\backslash$nthat if criteria commonly employed to demonstrate the equal mobility$\backslash$nof different grain sizes are used in the comparison, then Great Eggleshope$\backslash$nBeck is far closer to this condition in spite of its minimal development$\backslash$nof a pavement. It is concluded that the respective shapes of the$\backslash$nbed-material grain-size distributions, in particular their degrees$\backslash$nof skewness, are more important to the observed sorting patterns$\backslash$nthan are the effects of a pavement layer regulating grain entrainment$\backslash$nto produce an equal mobility of different grain sizes. Therefore,$\backslash$nthe comparison has established that flow-competence relationships$\backslash$nwill differ from one stream to another, depending on the pattern$\backslash$nof grain sorting which is a function of the bedmaterial grain-size$\backslash$ndistribution.},
author = {KOMAR, PAUL D. and CARLING, PAUL A.},
doi = {10.1111/j.1365-3091.1991.tb00363.x},
issn = {13653091},
journal = {Sedimentology},
number = {3},
pages = {489--502},
title = {{Grain sorting in gravel‐bed streams and the choice of particle sizes for flow‐competence evaluations}},
volume = {38},
year = {1991}
}
@article{Ferreira2015a,
abstract = {The nature of the von K{\'{a}}rm{\'{a}}n constant ($\kappa$), i.e. universal, Re-independent but wall-dependent or, in general, flow dependent, has been subjected to considerable investigation. Contradicting the classic view that $\kappa$ should be universal in open-channel flows of clear water with a fully developed boundary layer, irrespectively of the type of boundary, it has recently been proposed that the value of $\kappa$ is influenced by the mobility of the bottom boundary.Rather than advocating a particular view on the nature of $\kappa$, the objectives of this work are: (i) to provide a similarity framework for the nature of $\kappa$, highlighting, in the process, the underlying key phenomena at play, and (ii) to explore a theoretical model for $\kappa$ as a function of turbulence parameters.Dimensional analysis is performed, including a new key step, following the works of Selim Yalin, in which the shear-rate is interpreted as a two-phase variable, depending on flow as well as sediment parameters. Vaschy-Buckingham's theorem is applied and similarity arguments leading to the log-law are employed. These introduce appropriate hypothesis concerning complete and incomplete similarity, thus providing a theoretical framework for the discussion of the nature of $\kappa$. A theoretical model is then explored, materialised in a functional relation between $\kappa$ and turbulence parameters. The discussion of the nature of $\kappa$ becomes a discussion of the nature of those parameters, all susceptible to be measured. Such discussion is based on laboratory results concerning flows over porous immobile and mobile beds. The data reveals changes in the structure of turbulence for flows over weakly mobile gravel-sand beds but is not supportive of a reduction of the value of $\kappa$.},
author = {Ferreira, Rui M.L.},
doi = {10.1016/j.advwatres.2014.10.004},
file = {:home/kpierce/Documents/mendeley/Ferreira - 2015 - The von K{\'{a}}rm{\'{a}}n constant for flows over rough mobile beds. Lessons learned from dimensional analysis and similarity.pdf:pdf},
issn = {03091708},
journal = {Advances in Water Resources},
keywords = {Logarithmic law,Poorly-sorted sediment mixtures,Turbulence anisotropy,Von K{\'{a}}rm{\'{a}}n $\kappa$},
pages = {19--32},
publisher = {Elsevier Ltd},
title = {{The von K{\'{a}}rm{\'{a}}n constant for flows over rough mobile beds. Lessons learned from dimensional analysis and similarity}},
url = {http://dx.doi.org/10.1016/j.advwatres.2014.10.004},
volume = {81},
year = {2015}
}
@article{Chen2008,
abstract = {The bed material grain size distribution of gravel bed streams is often spatially heterogeneous. The heterogeneity is actually a random parameter, even for a '' well-mixed mixture,'' which potentially causes the transport rate for a given bed material to become an uncertain variable. The cause of bed material heterogeneity is the nonuniformity of the bed material, which is analyzed in this paper using examples from field observations and experimental data. The Monte Carlo simulation method is applied to study the uncertainty of the bedload transport rate using Wilcock's experimental data (P. R. Wilcock et al., 2001; P. R. Wilcock and J. C. Crowe, 2003). Each realization of the Monte Carlo simulation employed a randomly generated grain size distribution field for the entire simulation domain. With sufficient realizations the simulation results were adequate to show that the transport rates were a random variable and the mean transport rates did not fall on a single-valued curve when the local heterogeneity was taken into account. The results indicate that the bedload transport rate of nonuniform sediment has an intrinsic uncertainty that can result solely from the bed material. The results also partially account for the scatter of the fractional transport rate within Wilcock's experimental data. This study presents an important concept in understanding the uncertainty associated with estimates of sediment transport.},
author = {Chen, Li and Stone, Mark C},
doi = {10.1029/2006WR005483},
isbn = {0043-1397},
issn = {00431397},
journal = {Water Resources Research},
keywords = {doi:10.102,http://dx.doi.org/10.1029/2006WR005483},
number = {1},
pages = {1--11},
title = {{Influence of bed material size heterogeneity on bedload transport uncertainty}},
volume = {44},
year = {2008}
}
