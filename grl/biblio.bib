journal = {Advances in Water Resources},
keywords = {Coherent structure,Impulse criterion,Initiation of particle motion},
pages = {37--48},
publisher = {Elsevier Ltd},
title = {{Accounting for the role of turbulent flow on particle dislodgement via a coupled quadrant analysis of velocity and pressure sequences}},
url = {http://dx.doi.org/10.1016/j.advwatres.2017.01.005},
volume = {101},
year = {2017}
}
